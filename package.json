{
  "name": "bootstrap-admin",
  "description": "Open source responsive background management template based on bootstrap 5",
  "version": "2.0.1",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "docs:dev": "vitepress dev build/docs",
    "docs:build": "vitepress build build/docs",
    "docs:preview": "vitepress preview build/docs",
    "rollup": "rollup --config rollup.config.js  --bundleConfigAsCjs",
    "app.js": "gulp app.js",
    "js": "npm-run-all rollup app.js",
    "css": "gulp style",
    "img": "gulp img",
    "lib:move": "gulp move_lib move_formvalidation_to_lib",
    "lib:clean": "gulp clean_lib",
    "lib": "npm-run-all lib:move lib:clean",
    "css:fix": "gulp fix-css",
    "build": "npm-run-all js css img docs:build",
    "serve": "http-server . -p 9999 -c-1"
  },
