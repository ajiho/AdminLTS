{
  "dependencies": {
    "@ztree/ztree_v3": "^3.5.48",
    "better-scroll": "^2.5.0",
    "bootstrap-icons": "^1.9.1",
    "chart.js": "^3.8.0",
    "croppie": "^2.6.5",
    "dropzone": "^6.0.0-beta.2",
    "echarts": "^5.4.1",
    "jquery": "^3.6.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "select2": "^4.1.0-rc.0",
    "select2-bootstrap-5-theme": "^1.3.0",
    "sweetalert2": "^11.6.15",
    "tinymce": "^6.3.1"
  },
  "name": "bootstrap-admin",
  "description": "Open source responsive background management template based on bootstrap 5",
  "version": "1.1.0",
  "main": "dist/js/bsa.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "js-compile": "rollup --config rollup.config.js --sourcemap",
    "watch-js": "rollup --config rollup.config.js --sourcemap -w",
    "css-compile-bash": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js --output-style expanded --source-map true --source-map-contents true --precision 6 ",
    "css-compile": "npm run css-compile-bash -- build/scss/bootstrap-admin.scss dist/css/bootstrap-admin.css",
    "css-compile-splits": "npm run css-compile-bash -- build/scss/parts -o dist/css/alt/",
    "css-prefix": "postcss --config postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-prefix-splits": "postcss --config postcss.config.js --replace \"dist/css/alt/*.css\" \"!dist/css/alt/*.min.css\"",
    "css-minify-bash": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output ",
    "css-minify": "npm run css-minify-bash -- dist/css/ --batch --batch-suffix \".min\" \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify-splits": "npm run css-minify-bash -- dist/css/alt/ --batch --batch-suffix \".min\" \"dist/css/alt/*.css\" \"!dist/css/alt/*.min.css\""
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "autoprefixer": "^10.4.13",
    "clean-css-cli": "^5.6.1",
    "node-sass": "^8.0.0",
    "node-sass-package-importer": "^5.3.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.1.0",
    "rollup": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/ajiho/bootstrap-admin.git"
  },
  "keywords": [
    "bootstrap",
    "bootstrap5",
    "css",
    "responsive",
    "admin",
    "template",
    "control-panel",
    "dashboard"
  ],
  "homepage": "https://gitee.com/ajiho/bootstrap-admin",
  "author": "ajiho",
  "license": "MIT"
}
